    "ps-tree": "^1.1.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^3.0.6",
    "babel-cli": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.4.0",
    "eslint-config-kentcdodds": "^10.1.0",
    "husky": "^0.11.7",
    "jest-cli": "15.1.1",
